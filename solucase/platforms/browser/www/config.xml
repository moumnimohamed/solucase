<?xml version='1.0' encoding='utf-8'?>
<widget id="com.solucase.soluc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>solucase</name>
    <description>
        Hello World sample application that receive blog from a web site.
    </description>
    <author email="oualid.sobhi@solucase.com" href="http://solucase.com/">
        PhoneGap Team
    </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="orientation" value="default" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
